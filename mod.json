{
	"geode": "4.3.1",
	"version": "v1.1.3-alpha.1",
	"id": "bitz.customprofiles",
	"name": "CustomProfiles",
	"developer": "BitZ",
	"description": "A nice mod to customize your profile",
	"gd": {
		"android": "2.2074",
		"mac": "2.2074",
		"win": "2.2074",
		"ios": "2.2074"
	},
	"settings": {
		"Corners-section": {
			"name": "Corners",
			"description": "You can change the corners from your profile right there!",
			"type": "title"
		},
		"corner-options": {
			"name": "Placement",
			"descriptionold": "Change the corners right here.\n1 = <cy>Rounded</c> | 2 = <cy>Square</c>\n3 = <cy>None</c>",
			"type": "custom:corner-options",
			"default": 1
		},
		"Colors-section": {
			"name": "Colors",
			"description": "You can change the order of the colors from your profile right there!",
			"type": "title"
		},
		"color-options": {
			"name": "Placement",
			"descriptionold": "Change the colors right here.\n1 = <cy>Animate</c> | 2 = <cy>Invert</c>\n3 = <cy>Normal</c>",
			"type": "custom:color-options",
			"default": 3
		},
		"Extras-section": {
			"name": "Extras",
			"description": "Some extra options!",
			"type": "title"
		},
		"Disable-info-button": {
			"name": "Disable info button",
			"description": "This hide the info button from profile",
			"type": "bool",
			"default": false
		},
		"Account-settings": {
			"name": "",
			"type": "custom:account-settings"
		}

	},
	"tags": [
		"customization",
		"interface"
	],
	"links": {
		"source": "https://github.com/iArtie/CustomProfiles",
		"community": "https://discord.gg/gXg8w4xJDA"
	},
	"issues": {
		"info": "Post your issues on the <cp>Issue tracking repository</c>.",
		"url": "https://github.com/iArtie/CustomProfiles/issues/new/choose"
	},
	"dependencies": [
		{
			"id": "geode.node-ids",
			"version": ">=v1.11.0",
			"importance": "required"
		}
	]
}
